{
    "name": "macgriog/laravel-state-machine",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Mac Gregor",
            "email": "macgregor.porta@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.3",
        "symfony/property-access": "^4.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "@stable",
        "phploc/phploc": "^3.0",
        "sebastian/phpcpd": "^3.0",
        "pdepend/pdepend": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Macgriog\\StateMachine\\" : "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Macgriog\\StateMachine\\ServiceProvider"
            ]
        }
    }
}
